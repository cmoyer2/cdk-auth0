{
  "name": "@flit/cdk-auth0",
  "version": "1.3.0",
  "description": "A collection of AWS CDK constructs to manager your Auth0 resources programmatically",
  "keywords": [
    "aws",
    "cdk",
    "awscdk",
    "aws-cdk",
    "auth0",
    "oauth",
    "oauth2",
    "authentication"
  ],
  "homepage": "https://github.com/jumper-de/cdk-auth0",
  "bugs": "https://github.com/jumper-de/cdk-auth0/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/jumper-de/cdk-auth0.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Luis Vierroth",
    "email": "luis@jumper.de",
    "url": "https://github.com/p-mercury"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./src",
    "./.jsii",
    "./LICENSE.txt",
    "./README.md"
  ],
  "scripts": {
    "build": "jsii",
    "prepack": "jsii"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ],
    "tabWidth": 2,
    "overrides": [
      {
        "files": "*.sublime-project",
        "options": {
          "parser": "json",
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "auth0": "^4.3.1"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.552.0",
    "@types/aws-lambda": "^8.10.137",
    "@types/node": "^20.12.7",
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0",
    "jest": "^29.7.0",
    "jsii": "^5.4.0",
    "jsii-pacmak": "^1.97.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "prettier-plugin-svelte": "^3.2.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0"
  },
  "bundleDependencies": [
    "auth0"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jsii": {
    "outdir": "./dist",
    "excludeTypescript": [
      "**/handler.ts"
    ],
    "tsc": {
      "rootDir": "./src",
      "outDir": "./dist"
    },
    "targets": {
      "java": {
        "package": "flit.cdk-auth0",
        "maven": {
          "groupId": "flit",
          "artifactId": "cdk-auth0"
        }
      },
      "dotnet": {
        "namespace": "Flit.CDK.Auth0",
        "packageId": "Flit.CDK.Auth0"
      },
      "python": {
        "libName": "flit.cdk-auth0",
        "module": "flit.cdk-auth0",
        "classifiers": [
          "Framework :: AWS CDK",
          "Framework :: AWS CDK :: 2"
        ]
      }
    }
  },
  "stability": "stable"
}
